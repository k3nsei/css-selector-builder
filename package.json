{
  "name": "css-selector-builder",
  "version": "0.0.0",
  "private": false,
  "description": "Developer friendly CSS selector builder API",
  "homepage": "https://github.com/k3nsei/css-selector-builder",
  "bugs": {
    "url": "https://github.com/k3nsei/css-selector-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k3nsei/css-selector-builder.git"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Piotr StÄ™pniewski",
    "email": "k3nsei.pl@gmail.com",
    "url": "https://github.com/k3nsei"
  },
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "parcel build ./src/index.ts --cache-dir .cache/parcel",
    "build:watch": "parcel watch ./src/index.ts --cache-dir .cache/parcel",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "is-ci || husky install",
    "lint": "echo \"to be implemented\"",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "types-check": "tsc --noEmit",
    "types-check:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@parcel/validator-typescript": "2.0.0-beta.1",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.30",
    "husky": "5.0.9",
    "is-ci": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "parcel": "2.0.0-beta.1",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-packagejson": "2.2.9",
    "ts-jest": "26.5.1",
    "typedoc": "0.20.24",
    "typescript": "4.1.5"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": "please-use-yarn",
    "yarn": "1.22.10"
  },
  "targets": {
    "main": {
      "context": "node",
      "outputFormat": "commonjs",
      "isLibrary": true
    },
    "module": {
      "context": "node",
      "outputFormat": "esmodule",
      "isLibrary": true
    },
    "browser": {
      "context": "browser",
      "outputFormat": "esmodule",
      "isLibrary": true
    }
  }
}
